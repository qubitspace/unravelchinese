<%= concept("tag/tag_cell/show", @tag, current_user: current_user).call(:show) %>

<% @tag.taggings.each do |tagging| %>
  <% taggable_type = tagging.taggable.taggable_type.downcase %>
  <%= concept("#{taggable_type}/#{taggable_type}_cell/list", tagging.taggable, current_user: current_user) %>
<% end %>