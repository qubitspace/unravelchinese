<div class="show_word" word-id="<%=id%>" type="show">
  <div class="word_title"><%= simplified %></div>

  <h4>Definitions:</h4>
  <div id="definitions">
    <% definitions.each do |definition| %>
      <div class="word_definition">
          (<%= definition.rank %>) <%= definition.value %><br>
      </div>
    <% end %>
  </div>

  <h4>Tags:</h4>
  <div class="tags">
    <div class="tag">word</div>
    <div class="tag">misc</div>
    <div class="tag">computer programming</div>
    <div class="tag">test</div>
     <%#= concept("tagging/tagging_cell/show_tagging_cell", collection: taggings, current_user: current_user) %>
  </div>

  <h4>Actions:</h4>
  <% status_links.each do |link| %>
    <div class="link"><%= link %></div>
  <% end %>
</div>

